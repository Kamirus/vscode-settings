{
	// Place your snippets for coq here. Each snippet is defined under a snippet name and has a prefix, body and 
	// description. The prefix is what is used to trigger the snippet and the body will be expanded and inserted. Possible variables are:
	// $1, $2 for tab stops, $0 for the final cursor position, and ${1:label}, ${2:another} for placeholders. Placeholders with the 
	// same ids are connected.
	// Example:
	// "Print to console": {
	// 	"prefix": "log",
	// 	"body": [
	// 		"console.log('$1');",
	// 		"$2"
	// 	],
	// 	"description": "Log output to console"
  // }
  "Proof": {
    "prefix": "proof",
    "body": [
      "Proof.",
      "  $0",
      "  Qed."
    ]
  },
  "auto with arith.": { "prefix": "arithauto", "body": "auto with arith." },
  "·": { "prefix": "middot", "body": "· " },
  "inversion H1. clear H1. subst.": { "prefix": "ics", "body": "inversion $1. clear $1. subst. $0" },
  "absurd": { "prefix": "absurd", "body": "absurd" },
  "apply": { "prefix": "apply", "body": "apply" },
  "assumption": { "prefix": "assumption", "body": "assumption" },
  "congruence": { "prefix": "congruence", "body": "congruence" },
  "destruct": { "prefix": "destruct", "body": "destruct" },
  "exfalso": { "prefix": "exfalso", "body": "exfalso" },
  "exists": { "prefix": "exists", "body": "exists" },
  "forall": { "prefix": "forall", "body": "forall" },
  "injection": { "prefix": "injection", "body": "injection" },
  "intros": { "prefix": "intros", "body": "intros" },
  "inversion": { "prefix": "inversion", "body": "inversion" },
  "left": { "prefix": "left", "body": "left" },
  "reflexivity": { "prefix": "reflexivity", "body": "reflexivity" },
  "rewrite": { "prefix": "rewrite", "body": "rewrite" },
  "right": { "prefix": "right", "body": "right" },
  "simpl": { "prefix": "simpl", "body": "simpl" },
  "induction": { "prefix": "induction", "body": "induction" },
  "dependent": { "prefix": "dependent", "body": "dependent" },
  "dependent destruction": { "prefix": "depdesc", "body": "dependent destruction" },
